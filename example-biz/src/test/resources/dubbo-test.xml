<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="${dubbo.application.name}"/>
    <dubbo:annotation package="com.cnkonica.example"/>
    <dubbo:protocol name="dubbo" port="${dubbo.protocol.port}" host="${dubbo.protocol.host}"/>
    <dubbo:provider filter="traceId"/>
    <dubbo:registry address="${dubbo.registry.address}" client="curator"/>
    <dubbo:consumer check="false" lazy="true" retries="0"/>
    <import resource="classpath:dubbo-api-test.xml"/>
    <import resource="classpath:dubbo-ref-test.xml"/>
</beans>